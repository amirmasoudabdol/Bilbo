	number = {5},
	pages = {524-32},
	pmid = {22508865},
	pst = {ppublish},
	title = {Measuring the prevalence of questionable research practices with incentives for truth telling},
	volume = {23},
	year = {2012},
	Bdsk-Url-1 = {https://doi.org/10.1177/0956797611430953}}

